<h2> Create New Post </h2>

<%= form_with do |f| %>
<%= f.label :title, "Title of Post"  %>
<%= f.text_field :title %>
<br>
<br>
<%= f.label :body %>
<%= f.text_area :body, size: "50x10" %>
<br>
<br> 


<%= hidden_field_tag :authenticity_token, form_authenticity_token %>
<%= f.submit "Submit Post" %>

<% end %>